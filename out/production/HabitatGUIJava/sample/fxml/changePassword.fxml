<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9"
            fx:controller="sample.controller.ChangePassword">
    <children>
        <Label layoutX="67.0" layoutY="125.0" text="new password"/>
        <Label layoutX="39.0" layoutY="172.0" text="confirm new pass word"/>
        <Label fx:id="status" layoutX="198.0" layoutY="326.0"/>
        <Button fx:id="confirmButton" layoutX="79.0" layoutY="271.0" mnemonicParsing="false" onAction="#confirm"
                text="confirm"/>
        <Button fx:id="returnButton" layoutX="386.0" layoutY="271.0" mnemonicParsing="false"
                onAction="#returntoMyAccount" text="return to my account"/>
        <TextField fx:id="newpassword" layoutX="182.0" layoutY="134.0"/>
        <TextField fx:id="confirmnewpassword" layoutX="198.0" layoutY="167.0"/>
    </children>
</AnchorPane>
