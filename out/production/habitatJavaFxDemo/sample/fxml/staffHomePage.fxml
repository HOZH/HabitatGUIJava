<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="592.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9"
            fx:controller="sample.controller.StaffHomePage">
    <children>
        <Label layoutX="21.0" layoutY="39.0" text="Cancel request"/>
        <Label layoutX="34.0" layoutY="288.0" text="check item"/>
        <Label layoutX="30.0" layoutY="400.0"/>
        <Label layoutX="32.0" layoutY="409.0" text="wait to ship"/>
        <ListView fx:id="returnListView" layoutX="134.0" layoutY="20.0" prefHeight="200.0" prefWidth="416.0"/>
        <ListView fx:id="shipListView" layoutX="123.0" layoutY="336.0" prefHeight="151.0" prefWidth="416.0"/>
        <Button fx:id="checkitemButton" layoutX="216.0" layoutY="276.0" mnemonicParsing="false" onAction="#check"
                text="check items"/>
        <Button fx:id="signoutButton" layoutX="84.0" layoutY="543.0" mnemonicParsing="false" onAction="#signout"
                text="sign out"/>
        <Button fx:id="cancelButton" layoutX="197.0" layoutY="233.0" mnemonicParsing="false" onAction="#cancel"
                text="confirm cancel"/>
        <Button fx:id="shipButton" layoutX="384.0" layoutY="543.0" mnemonicParsing="false" onAction="#ship"
                text="ship "/>
        <Label layoutX="141.0" layoutY="506.0" text="add trucking number"/>
        <TextField fx:id="truckingTextField" layoutX="300.0" layoutY="501.0"/>
    </children>
</AnchorPane>
