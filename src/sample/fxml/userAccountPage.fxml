<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9"
            fx:controller="sample.controller.UserAccountPage">
    <children>
        <Button fx:id="orderHistoryButton" layoutX="215.0" layoutY="45.0" mnemonicParsing="false"
                onAction="#checkOrderHistory" text="order history"/>
        <Button fx:id="returnButton" layoutX="209.0" layoutY="220.0" mnemonicParsing="false" onAction="#returnHomePage"
                text="return to home"/>
        <Button fx:id="changeprofileButton" layoutX="214.0" layoutY="156.0" mnemonicParsing="false"
                onAction="#changeProfile" text="chang profile"/>
        <Button fx:id="signoutButton" layoutX="214.0" layoutY="280.0" mnemonicParsing="false" onAction="#signout"
                text="sign out"/>
        <Button fx:id="changepasswordButton" layoutX="215.0" layoutY="102.0" mnemonicParsing="false"
                onAction="#changePassword" text="change password"/>
    </children>
</AnchorPane>
